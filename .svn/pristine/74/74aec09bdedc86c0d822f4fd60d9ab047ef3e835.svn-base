	private String strHorVis; // HORA VISITA
	private String strTipVis; // TIPO
	private String strCodVis; // CÓDIGO VISITA
	
	public String getStrDocVta() {
		return strDocVta;
	}
	public void setStrDocVta(String strDocVta) {
		this.strDocVta = strDocVta;
	}
	
	public String getStrFecReg() {
		return strFecReg;
	}
	public void setStrFecReg(String strFecReg) {
		this.strFecReg = strFecReg;
	}
	public String getStrCodCli() {
		return strCodCli;
	}
	public void setStrCodCli(String strCodCli) {
		this.strCodCli = strCodCli;
	}
