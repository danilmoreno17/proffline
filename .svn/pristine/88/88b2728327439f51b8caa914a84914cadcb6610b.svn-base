package com.promesa.pedidos.bean;

public class BeanPedidoDetalle {

	private int tipo;
	private String posicion;
	private String material;
	private String cantidad;
	private String cantidadConfirmada;
	private String UM;
	private String porcentajeDescuento;
	private String denominacion;
	private String precioNeto;
	private String valorNeto;
	private String id_header = null;
	private String id = null;
	private String txtSincronizado = null;
	private String motivoRechazo;
	private String tipoSAP = "";
	private String marcadoParaActualizar = "";
	private Double dPesoNeto;

	public Double getdPesoNeto() {
		return dPesoNeto;
	}

	public void setdPesoNeto(Double dPesoNeto) {
		this.dPesoNeto = dPesoNeto;
	}

	public String getTxtSincronizado() {
		return txtSincronizado;
	}

	public void setTxtSincronizado(String txtSincronizado) {
		this.txtSincronizado = txtSincronizado;
	}

	public String getId_header() {
		return id_header;
	}

	public void setId_header(String id_header) {
		this.id_header = id_header;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPosicion() {
		return posicion;
	}

	public void setPosicion(String posicion) {
		this.posicion = posicion;
	}

	public String getMaterial() {
		return material;
	}

	public void setMaterial(String material) {
		this.material = material;
	}

	public String getCantidad() {
		return cantidad;
	}

	public void setCantidad(String cantidad) {
		this.cantidad = cantidad;
	}

	public String getCantidadConfirmada() {
		return cantidadConfirmada;
	}

	public void setCantidadConfirmada(String cantidadConfirmada) {
		this.cantidadConfirmada = cantidadConfirmada;
	}

	public String getUM() {
		return UM;
	}

	public void setUM(String uM) {
		UM = uM;
	}

	public String getPorcentajeDescuento() {
		return porcentajeDescuento;
	}

	public void setPorcentajeDescuento(String porcentajeDescuento) {
		this.porcentajeDescuento = porcentajeDescuento;
	}

	public String getDenominacion() {
		return denominacion;
	}

	public void setDenominacion(String denominacion) {
		this.denominacion = denominacion;
	}

	public String getPrecioNeto() {
		return precioNeto;
	}

	public void setPrecioNeto(String precioNeto) {
		this.precioNeto = precioNeto;
	}

	public String getValorNeto() {
		return valorNeto;
	}

	public void setValorNeto(String valorNeto) {
		this.valorNeto = valorNeto;
	}

	public int getTipo() {
		return tipo;
	}

	public void setTipo(int tipo) {
		this.tipo = tipo;
	}

	public String getMotivoRechazo() {
		return motivoRechazo;
	}

	public void setMotivoRechazo(String motivoRechazo) {
		this.motivoRechazo = motivoRechazo;
	}

	public String getTipoSAP() {
		return tipoSAP;
	}

	public void setTipoSAP(String tipoSAP) {
		this.tipoSAP = tipoSAP;
	}

	public String getMarcadoParaActualizar() {
		return marcadoParaActualizar;
	}

	public void setMarcadoParaActualizar(String marcadoParaActualizar) {
		this.marcadoParaActualizar = marcadoParaActualizar;
	}

	@Override
	public String toString() {
		return "BeanPedidoDetalle [tipo=" + tipo + ", posicion=" + posicion
				+ ", material=" + material + ", cantidad=" + cantidad
				+ ", cantidadConfirmada=" + cantidadConfirmada + ", UM=" + UM
				+ ", porcentajeDescuento=" + porcentajeDescuento
				+ ", denominacion=" + denominacion + ", precioNeto="
				+ precioNeto + ", valorNeto=" + valorNeto + ", id_header="
				+ id_header + ", id=" + id + ", txtSincronizado="
				+ txtSincronizado + ", motivoRechazo=" + motivoRechazo
				+ ", tipoSAP=" + tipoSAP + ", marcadoParaActualizar="
				+ marcadoParaActualizar + "]";
	}
}
