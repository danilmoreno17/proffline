package com.promesa.devoluciones.bean;

public class BeanMotivoDevolucion {

	private String strMandante;
	private String strIdioma;
	private String strCodMotDev;
	private String strMotDev;
	
	public String getStrMandante() {
		return strMandante;
	}
	public void setStrMandante(String strMandante) {
		this.strMandante = strMandante;
	}
	public String getStrIdioma() {
		return strIdioma;
	}
	public void setStrIdioma(String strIdioma) {
		this.strIdioma = strIdioma;
	}
	public String getStrCodMotDev() {
		return strCodMotDev;
	}
	public void setStrCodMotDev(String strCodMotDev) {
		this.strCodMotDev = strCodMotDev;
	}
	public String getStrMotDev() {
		return strMotDev;
	}
	public void setStrMotDev(String strMotDev) {
		this.strMotDev = strMotDev;
	}
}