package com.promesa.pedidos.bean;

public class BeanBloqueoEntrega {

	private String txtSincronizado;
	private String codigo;
	private String descripcion;

	public BeanBloqueoEntrega() {
		super();
	}

	public BeanBloqueoEntrega(String txtSincronizado, String codigo,
			String descripcion) {
		super();
		this.txtSincronizado = txtSincronizado;
		this.codigo = codigo;
		this.descripcion = descripcion;
	}

	public String getTxtSincronizado() {
		return txtSincronizado;
	}

	public void setTxtSincronizado(String txtSincronizado) {
		this.txtSincronizado = txtSincronizado;
	}

	public String getCodigo() {
		return codigo;
	}

	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}

	public String getDescripcion() {
		return descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

	@Override
	public String toString() {
		return descripcion;
	}

}
