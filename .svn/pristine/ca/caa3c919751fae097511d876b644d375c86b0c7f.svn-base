package com.promesa.pedidos.bean;

public class BeanClienteMaterial {
	
	private String condicionZPR1;
	private String codigoCliente;
	private String codigoMaterial;
	
	private double precioClienteMaterial;

	public void setCondicionZPR1(String condicionZPR1) {
		this.condicionZPR1 = condicionZPR1;
	}

	public String getCondicionZPR1() {
		return condicionZPR1;
	}

	public void setCodigoCliente(String codigoCliente) {
		this.codigoCliente = codigoCliente;
	}

	public String getCodigoCliente() {
		return codigoCliente;
	}

	public void setCodigoMaterial(String codigoMaterial) {
		this.codigoMaterial = codigoMaterial;
	}

	public String getCodigoMaterial() {
		return codigoMaterial;
	}

	public void setPrecioClienteMaterial(double precioClienteMaterial) {
		this.precioClienteMaterial = precioClienteMaterial;
	}

	public double getPrecioClienteMaterial() {
		return precioClienteMaterial;
	}
}
