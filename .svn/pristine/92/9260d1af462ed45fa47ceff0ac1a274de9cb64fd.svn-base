package com.promesa.administracion.bean;

import java.util.List;

public class BeanMenuUsuario {
	public List<BeanRol> getListaRol() {
		return listaRol;
	}
	public void setListaRol(List<BeanRol> listaRol) {
		this.listaRol = listaRol;
	}
	public List<BeanVista> getListaVista() {
		return listaVista;
	}
	public void setListaVista(List<BeanVista> listaVista) {
		this.listaVista = listaVista;
	}
	public List<BeanRolUsuario> getListaRolUsuario() {
		return listaRolUsuario;
	}
	public void setListaRolUsuario(List<BeanRolUsuario> listaRolUsuario) {
		this.listaRolUsuario = listaRolUsuario;
	}
	public List<BeanRolVista> getListaRolVista() {
		return listaRolVista;
	}
	public void setListaRolVista(List<BeanRolVista> listaRolVista) {
		this.listaRolVista = listaRolVista;
	}
	public List<BeanUsuario> getListaUsuario() {
		return listaUsuario;
	}
	public void setListaUsuario(List<BeanUsuario> listaUsuario) {
		this.listaUsuario = listaUsuario;
	}
	
	private List<BeanRol> listaRol;
	private List<BeanVista> listaVista; 
	private List<BeanRolUsuario> listaRolUsuario; 
	private List<BeanRolVista> listaRolVista; 
	private List<BeanUsuario> listaUsuario;
	
}