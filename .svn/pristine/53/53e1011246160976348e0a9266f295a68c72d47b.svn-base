	private String strDiaFeriado;
	private String strOriFeriado;
	
	public String getStrSigFeriado() {
		return strSigFeriado;
	}
	public void setStrSigFeriado(String strSigFeriado) {
		this.strSigFeriado = strSigFeriado;
	}
	
	public String getStrMesFeriado() {
		return strMesFeriado;
	}
	public void setStrMesFeriado(String strMesFeriado) {
		this.strMesFeriado = strMesFeriado;
