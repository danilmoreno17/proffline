package com.promesa.administracion.bean;

public class BeanIdentificacion {
	private String strId;
	private String strNombre;
	private String strC1; /* ZONA */
	private String strC2; /* ES O NO ES VENDEDOR */	

    public String getStrId() {
		return strId;
	}
	public void setStrId(String strId) {
		this.strId = strId;
	}
	public String getStrNombre() {
		return strNombre;
	}
	public void setStrNombre(String strNombre) {
		this.strNombre = strNombre;
	}
	public String getStrC1() {
		return strC1;
	}
	public void setStrC1(String strC1) {
		this.strC1 = strC1;
	}
	public String getStrC2() {
		return strC2;
	}
	public void setStrC2(String strC2) {
		this.strC2 = strC2;
	}
}